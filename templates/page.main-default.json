    "rich_text_gJq9yb": {
      "type": "rich-text",
      "blocks": {
        "heading_cFpbEt": {
          "type": "heading",
          "settings": {
            "heading": "Planting Materials Required",
            "heading_size": "h1"
          }
        },
        "text_ckrYyx": {
          "type": "text",
          "settings": {
            "text": "<p>Share information about your brand with your customers. Describe a product, make announcements, or welcome customers to your store.<\/p>"
          }
        },
        "text_XWzj8y": {
          "type": "text",
          "settings": {
            "text": "<ol><li><strong>Seed Flat - <\/strong>A standard 1020 seed flat with a solid bottom (no holes)<\/li><li><strong>Seed Flat Insert <\/strong>- A standard 1020 seed flat insert, with a mesh bottom (or holes), that tightly fits your 1020 seed flat (no holes).<\/li><li><strong>Cover - <\/strong>A standard 1020 seed flat insert, with a mesh bottom (or holes), that tightly fits your 1020 seed flat.<\/li><li><strong>Watering <\/strong>- A spray bottle, or a sink or shower head.<\/li><li><strong>Seeds - <\/strong>Your seeds of choice.<\/li><li><em><strong>ReNu Terra Classic Mats<\/strong><\/em> -  Ideal for microgreens & leafy greens.<\/li><li><em><strong>ReNu Terra Large Seed Format Mats<\/strong><\/em> - Ideal for deeper rooting plants like spinach & baby greens.<br\/><br\/><\/li><\/ol>"
          }
        }
      },
      "block_order": [
        "heading_cFpbEt",
        "text_ckrYyx",
        "text_XWzj8y"
      ],

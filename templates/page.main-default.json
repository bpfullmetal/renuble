    "image_with_text_wk9qj6": {
      "type": "image-with-text",
      "blocks": {
        "heading_B3Dh3c": {
          "type": "heading",
          "settings": {
            "heading": "1) Lay Mats in 1020 Tray",
            "heading_size": "h2"
          }
        },
        "text_EiCha3": {
          "type": "text",
          "settings": {
            "text": "<p>Remove one ReNu Terra mat from the packaging and lay it flat in your 1020 seed flat insert (with holes or mesh).<\/p><p>Place your 1020 seed flat insert in your 1020 flat (no holes).<\/p><p><br\/><\/p>",
            "text_style": "body"
          }
        }
      },
      "block_order": [
        "heading_B3Dh3c",
        "text_EiCha3"
      ],
      "settings": {
        "image": "shopify:\/\/shop_images\/1020_Tray_e42d51f3-cf21-4a50-8ef9-67897bf846bf.jpg",
        "height": "adapt",
        "desktop_image_width": "medium",
        "layout": "image_first",
